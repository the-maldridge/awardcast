{% extends "../../base.p2" %}

{% block title %}Award List{% endblock %}

{% block content %}
<div class="flex-container flex-row flex-center">
  <div class="flex-item flex-max foreground box">
    <form method="post" enctype="multipart/form-data">
      <label for="awards_file">Awards CSV</label>
      <input type="file" name="awards_file" id="awards_file" accept="text/csv" />
      <br />
      <input type="submit" />
    </form>
  </div>
</div>
{% endblock %}
